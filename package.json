{
    "name": "angular",
    "version": "2.15.0",
    "scripts": {
        "compile": "npm run npm:ci",
        "build": "npm run build:production",
        "production": "scully --project=angular --scanRoutes",
        "ng": "ng",
        "start": "ng serve --configuration development --port 4444",
        "build:dev": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration development",
        "build:production": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration production",
        "build:sourcemap": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --configuration source-map --source-map",
        "test:local": "ng test --watch=true",
        "test": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng test --no-watch --browsers=ChromeHeadless",
        "tsc": "tsc",
        "prettier": "prettier",
        "prettier:check": "npm run prettier -- --check \"**/*.{ts,md,html,scss}\"",
        "prettier:fix": "npm run prettier -- --write \"**/*.{ts,md,json,html,scss}\"",
        "pretty-quick": "pretty-quick",
        "pretty:fix": "pretty-quick -- --staged",
        "lint": "ng lint",
        "lint:fix": "ng lint --fix",
        "error-log": "cd error-log && npm ci && npm run start",
        "check": "npm run prettier:check && npm run lint",
        "fix": "npm run prettier:fix && npm run lint:fix",
        "npm:ci": "npm ci",
        "npm:i": "npm i && npx force-resolutions",
        "clean": "npm run clean:helpers && npm run clean:assets && npm run clean:node",
        "clean:helpers": "rm -rf ../shared/*.js && rm -rf ./shared/*.js.map && rm -rf ./shared/utils/*.js && rm -rf ./shared/utils/*.js.map ",
        "clean:assets": "rm -rf coverage-* dist/*",
        "clean:node": "rm -rf node_modules"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^15.2.9",
        "@angular/common": "^15.2.9",
        "@angular/compiler": "^15.2.9",
        "@angular/core": "^15.2.9",
        "@angular/forms": "^15.2.9",
        "@angular/localize": "^15.2.9",
        "@angular/platform-browser": "^15.2.9",
        "@angular/platform-browser-dynamic": "^15.2.9",
        "@angular/router": "^15.2.9",
        "@ng-bootstrap/ng-bootstrap": "^14.2.0",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "bootstrap": "^5.0.2",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.14.0",
        "core-js": "3.23.3",
        "cosmiconfig": "^7.0.1",
        "dotenv": "^10.0.0",
        "jsdom": "^17.0.0",
        "path": "^0.12.7",
        "rxjs": "^6.6.0",
        "semver": "^7.5.2",
        "tslib": "^2.1.0",
        "word-wrap": "^1.2.4",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1502.9",
        "@angular-devkit/build-angular": "^15.2.9",
        "@angular-eslint/builder": "^15.2.1",
        "@angular-eslint/eslint-plugin": "^15.2.1",
        "@angular-eslint/eslint-plugin-template": "^15.2.1",
        "@angular-eslint/schematics": "^15.2.1",
        "@angular-eslint/template-parser": "^15.2.1",
        "@angular/cli": "^15.2.9",
        "@angular/compiler-cli": "^15.2.9",
        "@commitlint/cli": "^17.5.0",
        "@commitlint/config-conventional": "^17.4.4",
        "@types/bootstrap": "^5.2.5",
        "@types/estree": "^1.0.0",
        "@types/jasmine": "^4.3.0",
        "@types/jsdom": "^16.2.13",
        "@types/node": "^18.11.9",
        "@scullyio/init": "^2",
        "@scullyio/ng-lib": "^2",
        "@scullyio/scully": "^2",
        "@scullyio/scully-plugin-puppeteer": "^2.1.41",
        "codecov": "^3.8.1",
        "debug": "^4.3.1",
        "engine.io": "^6.4.2",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "fs-extra": "^11",
        "husky": "^8.0.0",
        "jasmine-core": "^4.4.0",
        "jasmine-spec-reporter": "^5.0.0",
        "karma": "^6.3.12",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-jasmine": "^4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "prettier": "2.8.7",
        "prettier-eslint": "^15.0.1",
        "pretty-quick": "^3.1.0",
        "rollup": "^2.*",
        "socket.io": "^4.6",
        "source-map-explorer": "^2.5.3",
        "tough-cookie": "^4.1.3",
        "ts-node": "^10.2.1",
        "typescript": "4.8.4",
        "word-wrap": "^1.2.4",
        "xml2js": "0.5.0"
    },
    "resolutions": {
        "semver": "^7.5.2",
        "tough-cookie": "^4.1.3",
        "engine.io": "^6.4.2",
        "socket.io": "^4.6",
        "word-wrap": "^1.2.4",
        "xml2js": "0.5.0"
    },
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    },
    "engines": {
        "node": "18 || 20"
      },
      "browserslist": {
        "production": [
          ">0.2%",
          "not dead",
          "not op_mini all"
        ],
        "development": [
          "last 1 chrome version",
          "last 1 firefox version",
          "last 1 safari version"
        ]
      }
}
